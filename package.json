{
  "name": "postit",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for notifications",
  "main": "/server/server.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./server/app.js",
    "test": "NODE_ENV=travis sequelize db:migrate && NODE_ENV=travis nyc mocha server/test/* --timeout 20000",
    "test:dev": "NODE_ENV=test sequelize db:migrate:undo:all && sequelize db:migrate && nyc mocha server/test/* --timeout 10000",
    "test:client": "jest",
    "build": "babel server -d dist",
    "start:dev": "nodemon --watch server --exec babel-node -- ./server/bin/www",
    "start:pro": "NODE_ENV=production sequelize db:migrate:undo:all && sequelize db:migrate && ./node_modules/.bin/babel-node ./server/app.js",
    "dev": "nodemon server/server.js & webpack-dev-server",
    "webpack": "webpack-dev-server --port 3000 --hot",
    "app": "npm run start | npm run webpack",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "script": {
    "build": "babel --presets es2015 server -d dist",
    "test": "istanbul cover node_modules/mocha/bin/_mocha && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fob413/PostIt.git"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "server/test/"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/client/src/__test__"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ],
    "collectCoverage": true
  },
  "author": "Funsho Oluyole-Balogun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fob413/PostIt/issues"
  },
  "homepage": "https://github.com/fob413/PostIt#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
