{
  "name": "postit",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for notifications",
  "main": "/server/server.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./server/bin/www",
    "test": "mocha --compilers js:babel-core/register server/test/test.js --timeout 10000",
    "build": "babel server -d dist",
    "start:dev": "nodemon --watch server --exec babel-node -- ./server/bin/www",
    "dev": "nodemon server/server.js & webpack-dev-server",
    "webpack": "webpack-dev-server --port 3000 --hot",
    "app": "npm run start | npm run webpack"
  },
  "script": {
    "build": "babel --presets es2015 server -d dist",
    "test": "istanbul cover node_modules/mocha/bin/_mocha && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fob413/PostIt.git"
  },
